            </m:mrow>
          </m:math>
        </item>
        <item id="uid39">
          <m:math overflow="scroll">
            <m:mrow>
              <m:mi>R</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>I</m:mi>
                <m:mo>-</m:mo>
                <m:msup>
                  <m:mrow>
                    <m:mi mathvariant="bold">A</m:mi>
                  </m:mrow>
                  <m:mo>+</m:mo>
                </m:msup>
                <m:mi mathvariant="bold">A</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mi>N</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:msup>
                  <m:mrow>
                    <m:mi mathvariant="bold">A</m:mi>
                  </m:mrow>
                  <m:mo>+</m:mo>
                </m:msup>
